    "api:watch": "nodemon --exec tsx src/smartsocial/server/index.ts",
    "api:dev": "concurrently \"npm run dev\" \"npm run api:watch\"",
    "dev:full": "concurrently \"npm run dev\" \"npm run api\"",
    "set:cors": "gsutil cors set cors.json gs://datasenceai-c4e5f.firebasestorage.app",
    "get:cors": "gsutil cors get gs://datasenceai-c4e5f.firebasestorage.app",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "test:api": "curl http://localhost:3001/api/health",
    "test:quota": "curl http://localhost:3001/api/extension/v1/quota/mock"
